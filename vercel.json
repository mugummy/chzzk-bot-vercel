{
  "framework": null,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://web-production-19eef.up.railway.app/api/$1" },
    { "source": "/auth/(.*)", "destination": "https://web-production-19eef.up.railway.app/auth/$1" },
    { "source": "/(.*)\\.(js|css|png|jpg|jpeg|gif|svg|ico)", "destination": "/$1.$2" },
    { "source": "/dashboard/(.*)", "destination": "/index.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}